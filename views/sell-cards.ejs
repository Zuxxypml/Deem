  <body>
    <nav>
      <div class="logo-name">
        <div class="logo-image">
          <img src="Assets/img/logo.png" alt="" />
        </div>
        <span class="logo_name">Diamond Exchange</span>
      </div>

      <div class="menu-items">
        <ul class="nav-links">
          <li>
            <a href="/deem-home">
              <i class="uil uil-estate"></i>
              <span class="link-name">Dashboard</span>
            </a>
          </li>
          <li>
            <a href="/sell-coins">
              <i class="uil uil-exchange"></i>
              <span class="link-name">Sell Coins</span>
            </a>
          </li>
          <li>
            <a href="/sell-cards" class="active">
              <i class="uil uil-card-atm"></i>
              <span class="link-name">Sell Giftcards</span>
            </a>
          </li>
          <li>
            <a href="#">
              <i class="uil uil-bell"></i>
              <span class="link-name">Notifications</span>
            </a>
          </li>
          <li>
            <a href="#">
              <i class="uil uil-wallet"></i>
              <span class="link-name">Wallet</span>
            </a>
          </li>
          <li>
            <a href="#">
              <i class="uil uil-user"></i>
              <span class="link-name">My Account</span>
            </a>
          </li>
        </ul>

        <ul class="logout-mode">
          <li>
            <a href="/logout">
              <i class="uil uil-signout"></i>
              <span class="link-name">Logout</span>
            </a>
          </li>

          <li class="mode">
            <a href="#">
              <i class="uil uil-moon"></i>
              <span class="link-name">Dark Mode</span>
            </a>

            <div class="mode-toggle">
              <span class="switch"></span>
            </div>
          </li>
        </ul>
      </div>
    </nav>

